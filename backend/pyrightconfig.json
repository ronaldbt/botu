{
    "include": [
        "app"
    ],
    "exclude": [
        "**/node_modules",
        "**/__pycache__",
        "**/venv"
    ],
    "pythonVersion": "3.12",
    "pythonPlatform": "Darwin",
    "executionEnvironments": [
        {
            "root": ".",
            "pythonVersion": "3.12",
            "pythonPlatform": "Darwin",
            "extraPaths": [
                ".",
                "venv/lib/python3.12/site-packages"
            ]
        }
    ],
    "typeCheckingMode": "basic",
    "useLibraryCodeForTypes": true,
    "autoImportCompletions": true
}
